<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
           version="3.0">

    <servlet>
        <servlet-name>com.gwtbook.module.basic.Basic BasicService</servlet-name>
        <servlet-class>com.gwtbook.module.basic.server.BasicServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>com.gwtbook.module.basic.Basic BasicService</servlet-name>
        <url-pattern>/Basic/BasicService</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>/Basic.html</welcome-file>
    </welcome-file-list>
</web-app>
